# Get other shell configuration
source $HOME/.zsh/.zshenv
source $HOME/.zsh/.zsh.prompt          # set the prompt
source $HOME/.zsh/.zsh.path            # setup the PATH
source $HOME/.zsh/.zsh.manpath         # setup the MANPATH
source $HOME/.zsh/.zsh.aliases         # setup command aliases
source $HOME/.zsh/.utk                 # UTK default config
source $HOME/.zsh/.zsh-oh-my-zsh.stuff # Oh My ZSH Config
source $HOME/.zsh/.powerline9k         # Powerline Font
source $HOME/.zsh/.zlogout             # Logout Scripts
source $HOME/.zsh/.zsh-update          # PowerLevel9k update file
source $HOME/.zsh/.zshrc               # Personal ZSHRC
